              "id": "Time Keeping_Startup.createTimeKeepingSheet",
              "name": "Startup.createTimeKeepingSheet",
              "collectionId": "Time Keeping_Startup",
              "confirmBeforeExecute": false,
              "pluginType": "JS",
              "jsonPathKeys": [
                "async () => {\n  await getTimeKeepingSpreadsheet.run();\n  const spreadsheetData = getTimeKeepingSpreadsheet.data;\n  const currentTitle = moment().format('YYYY-MM');\n  if (!spreadsheetData.sheets.find(sheet => sheet.title === currentTitle)) {\n    await addTimeKeepingSheet.run({\n      title: currentTitle\n    });\n    await initTitleTimeKeepingSheet.run({\n      sheet: currentTitle\n    });\n  }\n  const nextTitle = moment().add(1, 'months').format('YYYY-MM');\n  if (!spreadsheetData.sheets.find(sheet => sheet.title === nextTitle)) {\n    await addTimeKeepingSheet.run({\n      title: nextTitle\n    });\n    await initTitleTimeKeepingSheet.run({\n      sheet: nextTitle\n    });\n  }\n}"
              ],
              "timeoutInMillisecond": 10000
